<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MTG">

    <Button
        android:id="@+id/butBack"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="230dp"
        android:text="Back"
        app:layout_constraintStart_toEndOf="@+id/verticalSeekBarWrapper4"
        app:layout_constraintTop_toTopOf="parent" />

    <com.h6ah4i.android.widget.verticalseekbar.VerticalSeekBarWrapper
        android:id="@+id/verticalSeekBarWrapper2"
        android:layout_width="21dp"
        android:layout_height="235dp"
        android:layout_marginStart="155dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">>

        <com.h6ah4i.android.widget.verticalseekbar.VerticalSeekBar
            android:id="@+id/BarG1"
            android:layout_width="wrap_content"
            android:layout_height="232dp"
            android:splitTrack="false"
            app:seekBarRotation="CW270" />
    </com.h6ah4i.android.widget.verticalseekbar.VerticalSeekBarWrapper>

    <com.h6ah4i.android.widget.verticalseekbar.VerticalSeekBarWrapper
        android:id="@+id/verticalSeekBarWrapper"
        android:layout_width="21dp"
        android:layout_height="235dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toEndOf="@+id/verticalSeekBarWrapper2"
        app:layout_constraintTop_toTopOf="parent">>

        <com.h6ah4i.android.widget.verticalseekbar.VerticalSeekBar
            android:id="@+id/BarG2"
            android:layout_width="wrap_content"
            android:layout_height="232dp"
            android:splitTrack="false"
            app:seekBarRotation="CW270" />
    </com.h6ah4i.android.widget.verticalseekbar.VerticalSeekBarWrapper>

    <com.h6ah4i.android.widget.verticalseekbar.VerticalSeekBarWrapper
        android:id="@+id/verticalSeekBarWrapper3"
        android:layout_width="21dp"
        android:layout_height="235dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toEndOf="@+id/verticalSeekBarWrapper"
        app:layout_constraintTop_toTopOf="parent">>

        <com.h6ah4i.android.widget.verticalseekbar.VerticalSeekBar
            android:id="@+id/BarG3"
            android:layout_width="wrap_content"
            android:layout_height="232dp"
            android:splitTrack="false"
            app:seekBarRotation="CW270" />
    </com.h6ah4i.android.widget.verticalseekbar.VerticalSeekBarWrapper>

    <com.h6ah4i.android.widget.verticalseekbar.VerticalSeekBarWrapper
        android:id="@+id/verticalSeekBarWrapper4"
        android:layout_width="21dp"
        android:layout_height="235dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toEndOf="@+id/verticalSeekBarWrapper3"
        app:layout_constraintTop_toTopOf="parent">>

        <com.h6ah4i.android.widget.verticalseekbar.VerticalSeekBar
            android:id="@+id/BarG4"
            android:layout_width="wrap_content"
            android:layout_height="232dp"
            android:splitTrack="false"
            app:seekBarRotation="CW270" />
    </com.h6ah4i.android.widget.verticalseekbar.VerticalSeekBarWrapper>

    <Switch
        android:id="@+id/swG1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:text="GROUP 1"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Switch
        android:id="@+id/swG2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="GROUP 2"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/swG1" />

    <Switch
        android:id="@+id/swG3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="GROUP 3"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/swG2" />

    <Switch
        android:id="@+id/swG4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="GROUP 4"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/swG3" />

    <TextView
        android:id="@+id/tvG1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="134dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvG2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="244dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvG3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="356dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvG4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="468dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>